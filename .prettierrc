{
	"overrides": [{ "files": "*.svelte", "options": { "parser": "svelte" } }],
	"plugins": ["prettier-plugin-svelte"],
	"pluginSearchDirs": ["."],
	"printWidth": 100,
	"semi": false,
	"singleQuote": true,
	"trailingComma": "all",
	"useTabs": true,
	"arrowParens": "always",
	"singleAttributePerLine": true
}
